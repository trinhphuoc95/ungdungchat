<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dinhtrinhphuoc.friendlychat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".GetStartedActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:parentActivityName=".ListUserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/loginUp_activity"
            android:parentActivityName=".GetStartedActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SignInActivity"
            android:label="@string/loginIn_activity"
            android:parentActivityName=".GetStartedActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ListUserActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>